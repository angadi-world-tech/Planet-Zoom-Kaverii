                            android:layout_height="45dp">
                            <ImageView
                                android:layout_width="23dp"
                                android:tint="@color/blue"
                                android:src="@mipmap/event_icon"
                                android:layout_height="23dp" />

                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
