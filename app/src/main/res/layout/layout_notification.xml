                        android:background="@drawable/orange_filled_circle"
                        android:layout_height="50dp">
                        <ImageView
                            android:layout_width="25dp"
                            android:tint="@color/white"
                            android:src="@mipmap/fav_icon"
                            android:layout_height="25dp" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        android:textSize="12dp"
                        android:text="@string/favorite"
                        android:layout_height="wrap_content" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="0.25"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:gravity="center"
                        android:visibility="gone"
                        android:id="@+id/LinlayoutBlueHistory"
                        android:background="@drawable/white_circle"
                        android:layout_height="50dp">
                        <ImageView
                            android:layout_width="25dp"
                            android:src="@mipmap/notification_icon"
                            android:layout_height="25dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="50dp"
                        android:gravity="center"
                        android:visibility="visible"
                        android:id="@+id/LinLayoutHistoryOrange"
                        android:background="@drawable/orange_filled_circle"
                        android:layout_height="50dp">
                        <ImageView
                            android:layout_width="25dp"
                            android:tint="@color/white"
                            android:src="@mipmap/notification_icon"
                            android:layout_height="25dp" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        android:textSize="12dp"
                        android:text="@string/notification"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="0.25"
                    android:gravity="center"
                    android:id="@+id/trip"
                    android:visibility="visible"
                    android:orientation="vertical"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:gravity="center"
                        android:background="@drawable/white_circle"
                        android:layout_height="50dp">
                        <ImageView
                            android:layout_width="25dp"
                            android:src="@mipmap/trip_management_icon"
                            android:layout_height="25dp" />

                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        android:textSize="9dp"
                        android:text="@string/trip_management"
                        android:layout_height="wrap_content" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="0.25"
                    android:gravity="center"
                    android:visibility="gone"
                    android:id="@+id/trip_orange"
                    android:orientation="vertical"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:gravity="center"
                        android:background="@drawable/orange_filled_circle"
                        android:layout_height="50dp">
                        <ImageView
                            android:layout_width="25dp"
                            android:tint="@color/white"
                            android:src="@mipmap/trip_management_icon"
                            android:layout_height="25dp" />

                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:textColor="@color/orange"
                        android:textSize="9dp"
                        android:text="@string/trip_management"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="0.3"
                android:weightSum="1.0"
                android:orientation="horizontal"
                android:layout_height="0dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:gravity="top|center"
                    android:orientation="horizontal"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/help_border"
                    android:layout_weight="0.25"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:text="@string/help"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginRight="4dp"
                        android:textColor="@color/blue"
                        android:textSize="12dp"
                        android:layout_height="wrap_content" />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_marginLeft="4dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:src="@mipmap/help"
                        android:layout_height="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_height="match_parent"></LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:gravity="top|center"
                    android:orientation="horizontal"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/unread_border"
                    android:layout_weight="0.25"
                    android:layout_height="match_parent">


                    <ImageView
                        android:layout_width="15dp"
                        android:layout_marginRight="4dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:src="@mipmap/envolope_blue_icon"
                        android:layout_height="15dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:text="@string/unread"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginLeft="4dp"
                        android:textColor="@color/blue"
                        android:textSize="12dp"
                        android:layout_height="wrap_content" />



                </LinearLayout>


            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

</RelativeLayout>